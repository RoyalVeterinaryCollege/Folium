		"Microsoft.Extensions.Logging": "1.0.0",
		"Microsoft.Extensions.Logging.Console": "1.0.0",
		"Microsoft.Extensions.Logging.Debug": "1.0.0",
		"Microsoft.AspNetCore.AngularServices": "1.0.0-*",
		"IdentityServer4.AccessTokenValidation": "1.0.2",
		"IdentityModel": "2.5.1",
		"Dapper": "1.50.2",
		"CsvHelper": "2.15.0.2",
		"ImageSharp": "1.0.0-alpha2-00117",
		"ImageSharp.Formats.Jpeg": "1.0.0-alpha2-00117",
		"ImageSharp.Formats.Bmp": "1.0.0-alpha2-00117",
		"ImageSharp.Formats.Png": "1.0.0-alpha2-00113",
		"ImageSharp.Processing": "1.0.0-alpha2-00105",
		"Autofac.Extensions.DependencyInjection": "4.0.0",
		"Serilog": "2.4.0",
		"Serilog.Extensions.Logging": "1.4.0",
		"Serilog.Sinks.RollingFile": "3.3.0",
